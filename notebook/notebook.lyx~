\begin_inset Tabular
<lyxtabular version="3" rows="14" columns="1">
<features>
<column alignment="left" valignment="top" width="0">
<row>
<cell alignment="left" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula ${n \choose k}=\frac{n!}{(n-k)!k!}$
\end_inset

 
\end_layout

\end_inset
</cell>
</row>
<row>
<cell alignment="none" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula ${n \choose k}={n-1 \choose k}+{n-1 \choose k-1}$
\end_inset

 
\end_layout

\end_inset
</cell>
</row>
